{
  "expo": {
    "name": "persuasive-mhealth-rn",
    "slug": "persuasive-mhealth-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/whiteLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#7e22ce"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
      "foregroundImage": "./assets/whiteLogo.png",
      "backgroundColor": "#7e22ce"
      },
      "permissions": [
      "android.permission.RECORD_AUDIO",
      "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    }
    },
    "web": {
      "favicon": "./assets/whiteLogoIcon.png"
    },
    "plugins": [
      "expo-audio",
      "expo-video"
    ],
    "extra": {
    
  }
}
